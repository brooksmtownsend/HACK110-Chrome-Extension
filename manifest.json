{
"manifest_version": 2,
    "name": "HACK 110 Text Replacement Extension",
    "description": "We can replace any text on a webpage with better text!",
    "version": "1.0",
    "icons": {
      "74": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
